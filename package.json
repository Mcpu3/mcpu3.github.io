{
  "devDependencies": {
    "bulma": "^0.9.4",
    "bulma-extensions": "^6.2.7",
    "es-module-shims": "^1.6.1",
    "node-sass": "^7.0.3",
    "three": "^0.145.0"
  },
  "scripts": {
    "index-css-build": "node-sass --omit-source-map-url ./sass/index.scss ./css/bulma/index.css",
    "bio-css-build": "node-sass --omit-source-map-url ./sass/bio.scss ./css/bulma/bio.css",
