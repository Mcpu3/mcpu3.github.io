{
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^6.2.0",
    "bulma": "^0.9.4",
    "bulma-extensions": "^6.2.7",
    "es-module-shims": "^1.6.1",
    "node-sass": "^7.0.3",
    "npm-run-all": "^4.1.5",
    "three": "^0.145.0"
  },
  "scripts": {
    "build_index_css": "node-sass --omit-source-map-url ./sass/index.scss ./css/bulma/index.css",
    "build_bio_css": "node-sass --omit-source-map-url ./sass/bio.scss ./css/bulma/bio.css",
    "build_wordle_solver_css": "node-sass --omit-source-map-url ./sass/wordle_solver.scss ./css/bulma/wordle_solver.css",
    "start": "npm-run-all build_index_css build_bio_css build_wordle_solver_css"
  }
}
